  "scripts": {
    "dev": "concurrently \"vite --config vite.config.local.ts\" \"tsx server/index.ts\"",
    "build": "vite build --config vite.config.local.ts && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
